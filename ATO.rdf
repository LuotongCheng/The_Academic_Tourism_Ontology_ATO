<rdf:Description rdf:about="http://iflastandards.info/ns/lrm/lrmoo/R2_is_derivative_of">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
	<rdfs:subPropertyOf rdf:resource="http://iflastandards.info/ns/lrm/lrmoo/R68_is_inspired_by"/>
	<owl:inverseOf rdf:resource="http://iflastandards.info/ns/lrm/lrmoo/R2i_has_derivative"/>
